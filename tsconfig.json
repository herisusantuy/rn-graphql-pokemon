{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@contexts/*": ["src/contexts/*"],
      "@navigations/*": ["src/navigations/*"],
      "@screens/*": ["src/screens/*"],
      "@hooks/*": ["src/hooks/*"],
      "@utils/*": ["src/utils/*"],
      "@components/*": ["src/components/*"],
      "@services/*": ["/src/services/*"]
    }
  }
}
